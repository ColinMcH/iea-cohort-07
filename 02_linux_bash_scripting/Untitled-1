
import random
number = random.randint(1, 100)

user_guess = int(input("Guess a number: "))


while True: 
    user_guess = int(input("Guess a number: "))
    
    if user_guess > number:
        print(" it's too high")
    elif user_guess < number:
        print(" it's too low")
    else:
        print("guess is correct")